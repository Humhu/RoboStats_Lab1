<log_reader>
	<log_path>robotdata1.log</robot_log_path>
	<scale>0.1</scale>
</log_reader>

<map>
	<map_path>wean.dat</map_path>
</map>

<particle_filter>
	<sensor_model>gaussian</sensor_model>
	<transition_model>
		<type>default</type>
		<var_x>1.0</var_x>
		<var_y>1.0</var_y>
		<var_t>1.0</var_t>
	</transition_model>
	<resampler>standard</resampler>
	
	<num_particles>1000</num_particles>
</particle_filter>

<filter_visualizer>
	<window_name>Filter Visualizer</window_name>
	<map_scale>1.0</map_scale>
	<robot_size>10.0</robot_size>
	<particle_subsample>1</particle_subsample>
</filter_visualizer>